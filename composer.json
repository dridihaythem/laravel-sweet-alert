{
    "name": "dridihaythem/laravel-sweet-alert",
    "description": "A simple PHP package to show SweetAlerts with the Laravel Framework in just one line and without any Javascript code.",
    "keywords": ["laravel", "SweetAlert"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DridiHaythem\\LaravelSweetAlert\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Haythem Dridi",
            "email": "haithemdridiweb@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DridiHaythem\\LaravelSweetAlert\\SweetAlertProvider"
            ],
            "aliases": {
                "SweetAlert": "DridiHaythem\\LaravelSweetAlert\\Facades\\SweetAlert"
            }
        }
    }
}
